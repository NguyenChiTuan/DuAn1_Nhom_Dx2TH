<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.javaweb.controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.javaweb.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BinhLuanSanPhamServlet</servlet-name>
        <servlet-class>com.javaweb.controller.BinhLuanSanPhamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThanhtoanSeverlet</servlet-name>
        <servlet-class>com.javaweb.controller.ThanhtoanSeverlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DanhMucQuanLySanPham</servlet-name>
        <servlet-class>com.javaweb.controller.DanhMucQuanLySanPham</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loaddmqlsp</servlet-name>
        <servlet-class>com.javaweb.controller.loaddmqlsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUserServlet</servlet-name>
        <servlet-class>com.javaweb.controller.EditUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XoaNhieuUserServlet</servlet-name>
        <servlet-class>com.javaweb.controller.XoaNhieuUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XoaAllSPServlet</servlet-name>
        <servlet-class>com.javaweb.controller.XoaAllSPServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThemSanPham</servlet-name>
        <servlet-class>com.javaweb.controller.ThemSanPham</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThemSanPhamServlet</servlet-name>
        <servlet-class>com.javaweb.controller.ThemSanPhamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuaSanPhamServlet</servlet-name>
        <servlet-class>com.javaweb.controller.SuaSanPhamServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>XoaNhieuSPServlet</servlet-name>
        <servlet-class>com.javaweb.controller.XoaNhieuSPServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditSPServlet</servlet-name>
        <servlet-class>com.javaweb.controller.EditSPServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loaddmqltt</servlet-name>
        <servlet-class>com.javaweb.controller.loaddmqltt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BinhLuanSanPhamServlet</servlet-name>
        <url-pattern>/BinhLuanSanPhamServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThanhtoanSeverlet</servlet-name>
        <url-pattern>/ThanhtoanSeverlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DanhMucQuanLySanPham</servlet-name>
        <url-pattern>/DanhMucQuanLySanPham</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loaddmqlsp</servlet-name>
        <url-pattern>/loaddmqlsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUserServlet</servlet-name>
        <url-pattern>/EditUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XoaNhieuUserServlet</servlet-name>
        <url-pattern>/XoaNhieuUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XoaAllSPServlet</servlet-name>
        <url-pattern>/XoaAllSPServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThemSanPham</servlet-name>
        <url-pattern>/ThemSanPham</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThemSanPhamServlet</servlet-name>
        <url-pattern>/ThemSanPhamServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuaSanPhamServlet</servlet-name>
        <url-pattern>/SuaSanPhamServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>XoaNhieuSPServlet</servlet-name>
        <url-pattern>/XoaNhieuSPServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditSPServlet</servlet-name>
        <url-pattern>/EditSPServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loaddmqltt</servlet-name>
        <url-pattern>/loaddmqltt</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <context-param>
        <description>Location to store uploaded file</description>
        <param-name>file-upload</param-name>
        <param-value>
            images\
        </param-value>
    </context-param>
    <welcome-file-list>
        <welcome-file>faces/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
